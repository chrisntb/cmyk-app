{
	"author": "chrisntb",
	"engines": {
		"node": ">=v24.12.x",
		"npm": ">=11.7.x"
	},
	"name": "cmyk-app",
	"private": true,
	"repository": {
		"type": "git",
		"url": "git@github.com:chrisntb/cmyk-app_.git"
	},
	"type": "module",
	"version": "0.0.1",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",

		"sk": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"sk:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",

		"format": "biome format",
		"format_": "biome format --write",
		"check": "biome check",
		"check_": "biome check --write",
		"lint": "biome lint",
		"lint_": "biome lint --write",

		"test": "vitest run",
		"test:watch": "vitest",
		"coverage": "vitest run --coverage"
	},
	"devDependencies": {
		"@biomejs/biome": "2.3.15",
		"@internationalized/date": "3.11.0",
		"@lucide/svelte": "0.564.0",
		"@sveltejs/adapter-auto": "7.0.1",
		"@sveltejs/kit": "2.51.0",
		"@sveltejs/vite-plugin-svelte": "6.2.4",
		"@tailwindcss/forms": "0.5.11",
		"@tailwindcss/vite": "4.1.18",
		"@types/node": "24.10.13",
		"@vitest/coverage-v8": "4.0.18",
		"bits-ui": "2.15.5",
		"clsx": "2.1.1",
		"globals": "17.3.0",
		"svelte": "5.51.0",
		"svelte-check": "4.4.0",
		"tailwind-merge": "3.4.0",
		"tailwind-variants": "3.2.2",
		"tailwindcss": "4.1.18",
		"tw-animate-css": "1.4.0",
		"typescript": "5.9.3",
		"vite": "7.3.1",
		"vitest": "4.0.18"
	},
	"dependencies": {
		"@sveltejs/adapter-static": "3.0.10"
	}
}
